@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<section id="only-one-message">
    <div class="container">
        <div class="box">
            <h1 class="title">@ViewData["Title"]</h1>
            <p class="message">You do not have access to this resource.</p>
        </div>
    </div>
</section>

<link rel="stylesheet" href="~/css/identity/OnlyOneMessage.css" asp-append-version="true"/>
